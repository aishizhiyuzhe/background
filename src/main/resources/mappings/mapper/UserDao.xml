<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aishizhiyuzhe.mapper.UserDao">

    <resultMap id="UserResultMap" type="com.aishizhiyuzhe.entity.User">
        <result property="loginName" column="login_Name" />
        <result property="password" column="password" />
    </resultMap>
    <select id="selectUser" resultMap="UserResultMap" parameterType="String">
        select * from sys_user where 1=1 and login_name = #{loginName}
    </select>

    <insert id="insert">
        insert into sys_user values ('aa','aa')
    </insert>
</mapper>